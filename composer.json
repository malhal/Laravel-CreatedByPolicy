    "keywords": ["laravel", "framework", "security", "permissions", "createdBy", "updatedBy", "user", "models", "CreatedByPolicy", "Laravel Security", "Laravel-CreatedByPolicy", "Malcolm Hall", "malhal"],
    "license": "MIT",
    "authors": [
        {
            "name": "Malcolm Hall",
            "email": "composer@malcolmhall.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
		"malhal/createdby": "dev-master"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Malhal\\CreatedByPolicy\\": "src/"
        }
    },
    "autoload-dev": {
    },
    "config": {
